
{
  "compilerOptions": {
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noResolve": true,
    "allowUnusedLabels": true,
    "skipLibCheck": true,
    "noEmitOnError": true,
    "strict": false,
    "removeComments": false,
    "module": "CommonJS",
    "target": "ES2015",
    "alwaysStrict": false
  }
}
